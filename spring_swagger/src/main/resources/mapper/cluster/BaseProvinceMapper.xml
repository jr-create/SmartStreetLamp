<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wjr.spring_swagger.mapper.cluster.BaseProvinceMapper">

    <select id="findProvinceByName" resultType="com.wjr.spring_swagger.bean.dim.BaseProvince">
        select * from base_province
        where name=#{name}
    </select>

    <select id="findAll" resultType="com.wjr.spring_swagger.bean.dim.BaseProvince">
        select * from base_province
    </select>
</mapper>
